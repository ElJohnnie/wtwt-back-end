{
    "env": {
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": ["standard"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "object-property-newline": "error",
        "no-unused-vars": "off",
        "prefer-const": "off",
        "object-shorthand": "off",
        "indent": ["error", 4]
    }
}